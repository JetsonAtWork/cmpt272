{
    "compilerOptions": {
      "target": "ESNext",
      "jsx": "react-jsx",
      "skipLibCheck": true,
      "checkJs": true,
      "allowJs": true,
      "baseUrl": ".",
      /* Bundler mode */
      "moduleResolution": "Bundler",
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "module": "ESNext",
      "types": ["vite/client"],
      "paths": {
        "@/*": ["./src/*"]
      },
      "strictNullChecks": false,
      "noImplicitAny": false,
      "exactOptionalPropertyTypes": false,
      "allowSyntheticDefaultImports": true,
    },
    "typeAcquisition": {
      "include": [
        "react-router-dom",
        "node"
      ],
      "enable": true
    },    
    "include": ["src"],
    "exclude": ["node_modules", "**/node_modules/*", "dist"]
} 
